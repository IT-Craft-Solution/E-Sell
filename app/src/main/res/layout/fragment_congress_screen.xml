<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.CongressScreenFragment">


   <com.airbnb.lottie.LottieAnimationView
       android:id="@+id/lottieCongressAnimation"
       android:layout_width="match_parent"
       android:layout_height="@dimen/_350mdp"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent"
       app:lottie_autoPlay="true"
       app:lottie_rawRes="@raw/congratulation" />


   <Button
       android:id="@+id/btnPreviewAds"
       android:layout_width="0dp"
       android:layout_height="wrap_content"
       android:layout_margin="@dimen/_16mdp"
       android:text="Preview Ads"
       app:layout_constraintBottom_toBottomOf="parent"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintHorizontal_bias="0.498"
       app:layout_constraintStart_toStartOf="parent" />

   <TextView
       android:id="@+id/textView4"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="24dp"
       android:text="Congratulation !!"
       android:textColor="@color/black"
       android:textSize="@dimen/_20font_mdp"
       android:textStyle="bold"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/lottieCongressAnimation" />

   <TextView
       android:id="@+id/textView5"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="16dp"
       android:text="Your Ad Will Successfully Posted..."
       android:textColor="@color/black"
       android:textSize="@dimen/_14font_mdp"
       app:layout_constraintEnd_toEndOf="@+id/textView4"
       app:layout_constraintHorizontal_bias="0.507"
       app:layout_constraintStart_toStartOf="@+id/textView4"
       app:layout_constraintTop_toBottomOf="@+id/textView4" />


</androidx.constraintlayout.widget.ConstraintLayout>